<!-- Core Meta -->
<meta charset="utf-8" />
<meta name="viewport" content="width=device-width,initial-scale=1" />
<!-- SEO Meta -->
<meta
  name="description"
  content="CollabSpace - Team collaboration platform with real-time chat, task management, and team communication"
/>
<meta
  name="keywords"
  content="team collaboration, project management, real-time chat, task tracking, team communication"
/>
<meta name="author" content="CollabSpace Team" />
<!-- Open Graph -->
<meta property="og:title" content="CollabSpace - Team Collaboration Platform" />
<meta
  property="og:description"
  content="The ultimate team collaboration platform that brings your team together with real-time chat, task management, and seamless communication."
/>
<meta property="og:type" content="website" />
<meta property="og:url" content="https://collabspace.com" />
<!-- Twitter -->
<meta name="twitter:card" content="summary_large_image" />
<meta
  name="twitter:title"
  content="CollabSpace - Team Collaboration Platform"
/>
<meta
  name="twitter:description"
  content="The ultimate team collaboration platform that brings your team together with real-time chat, task management, and seamless communication."
/>

<title><%= typeof title !== 'undefined' ? title : 'CollabSpace' %></title>

<!-- Favicon (optional placeholder) -->
<link rel="icon" type="image/svg+xml" href="/favicon.svg" />

<!-- Bootstrap CSS -->
<link
  href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css"
  rel="stylesheet"
  integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9"
  crossorigin="anonymous"
/>
<!-- Font Awesome -->
<link
  href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css"
  rel="stylesheet"
  integrity="sha512-pbEqX3ZQd0iWqNQ6FJ+0XkS7LxyzKDn5XxhxL7sRKqzZo4PMBVXrS5aXoaZySUdkGFUTkOcJCXyZ1qf7o1JpNQ=="
  crossorigin="anonymous"
  referrerpolicy="no-referrer"
/>
<!-- Main Stylesheet -->
<link rel="stylesheet" href="/css/style.css" />
<!-- Page Specific Extra Styles Hook -->
<%- typeof extraStyles !== 'undefined' ? extraStyles : '' %>
